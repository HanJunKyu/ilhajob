html {
  position: relative;

  min-height: 100%;

  margin: 0;
}

body {
  min-height: 100%;
}



/* mypage */
.my-menu-container {
  display: flex;
  justify-content: center;
  background-color: white;
  margin-bottom: 30px;
}

.my-menu {
  list-style: none;
  display: flex;
  margin-top: 20px;
  padding-left: 0;
}

.my-menu li {
  padding: 0;
}

@media (max-width: 736px) {
  .my-menu {
    margin-top: 20px;
  }
}
@media (max-width: 480px) {
  .my-menu {
    margin-top: 10px;
  }
  .my-menu-container {
    margin-bottom: 0;
  }

  .resume-edit {
    margin-top: 0 !important;
  }
}

/* myapply */

.menu {
  color: rgb(66, 66, 66) !important;
  font-size: 22px;
  padding: 0 6.5vw;
  padding-bottom: 20px;
  border-bottom: 2px solid #f2f2f2;
}

.menu-on {
  font-size: 22px;
  padding: 0px 6.5vw;
  padding-bottom: 20px;
  border-bottom: 3px solid rgb(55, 179, 127);
}

.apply-table td {
  border: 1px solid #d9d9d9;
  vertical-align: middle;
}

.apply-table-wrapper {
  border-top: 2px solid rgb(66, 66, 66);
  display: flex;
  margin: 0 auto;
  width: 85vw;
  margin-top: 10px;
}

.apply-table {
  padding-top: 10px;
  border: 1px solid #d9d9d9;
  padding-left: 40px;
  font-size: 15px;
  font-weight: 400;
  color: rgb(66, 66, 66);
  width: 100%;
}

.apply-table tbody {
  text-align: center;
}

.apply-menu {
  font-weight: 600;
  background-color: rgba(242, 242, 242, 0.5);
}

#apply_title, #apply_company a {
  color: rgb(66, 66, 66) !important;
}

.apply-table p {
  margin-bottom: 0;
  line-height: 15px;
}

.apply-read {
  color: rgb(55, 179, 127);
}

a:hover {
  color: rgb(230, 104, 66) !important;
}

@media (max-width: 768px) {
  .apply-table-wrapper {
    width: 90vw;
    margin-top: 25px;
  }

  .apply-table td {
    font-size: 12px;
  }
}

@media (max-width: 480px) {
  .apply-table-wrapper {
    width: 95vw;
    margin-top: 10px !important;
  }

  .apply-table td {
    font-size: 7px;
    line-height: 1.4;
  }
}

/* mymain */

.list-container {
  display: flex;
  justify-content: center;
  margin-bottom: 30px;
}

.name {
  font-size: 20px;
  margin-top: 15px;
}

.list-div {
  width: 85vw;
  height: 300px;
  display: flex;
  padding-top: 15px;
}

.list {
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  width: 11.5vw;
  margin: 0 5vw;
}

.circle-image {
  width: 11.5vw;
  height: 11.5vw;
  border-radius: 50%;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #d9d9d9;
}

.circle {
  background-color: #f2f2f2;
  width: 11.5vw;
  height: 11.5vw;
  border-radius: 50%;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}

.circle p {
  font-size: 45px;
  font-weight: 500;
  color: rgb(41, 41, 41);
  margin-bottom: 0;
}

.list > p {
  font-size: 20px;
  margin-top: 15px;
}

.recommend {
  width: 80vw;
  margin: 0 auto;
  margin-bottom: 80px;
}

.recommend-header {
  justify-content: center;
  height: 50px;
  display: flex;
}

.span-div {
  width: 13%;
  /* padding: 13px 4.9% !important; */
  font-size: 20px;
  border: 1px solid rgb(41, 41, 41);
  border-bottom-style: none;
  color: rgb(66, 66, 66);
  display: flex;
  justify-content: center;
  align-items: center;
}

.recommend-header .div {
  border-bottom: 1px solid rgb(41, 41, 41);
  width: 87%;
}

.recommend-table-wrapper table {
  margin: 0 auto;
  margin-top: 10px;
}
.recommend-table-wrapper {
  border-bottom: 1px solid rgb(41, 41, 41) !important;
}

.recommend-table-wrapper table td {
  vertical-align: middle;
  padding: 15px 0;
}

.recommend-table-wrapper #recommend_company {
  padding: 0 20px;
  font-weight: 600;
  font-size: 18px;
}

.recommend-table-wrapper tbody:first-child tr {
  border-top-style: none;
}

.recommend-table-wrapper #recommend_title {
  font-weight: 600;
  font-size: 18px;
}

.recommend-info {
  font-size: 13px;
}

.recommend-info span {
  margin: 0;
  padding-right: 18px;
}

.recommend-info i {
  margin-right: 5px;
  color: #808080;
}

#recommend_date {
  font-size: 16px;
}

.recommend-table-wrapper .button-div {
  text-align: right;
}

.recommend-table-wrapper .primary {
  width: 150px;
  background-color: rgb(230, 104, 66) !important;
}

@media (min-width: 481px) and (max-width: 970px) {
  .span-div {
    width: 20%;
  }

  .recommend-header .div {
    width: 80%;
  }

  .recommend {
    width: 90vw;
  }

  .recommend-table-wrapper table {
    margin: 0 auto;
    margin-top: 10px;
    margin-left: 10px;
    margin-right: 10px;
  }

  .recommend-table-wrapper table td {
    padding: 0;
    display: block;
    text-align: left;
  }

  .recommend-table-wrapper #recommend_company {
    padding: 0;
    font-size: 17px;
    font-weight: normal;
  }

  .recommend-table-wrapper #recommend_title {
    color: rgb(61, 61, 61);
    font-size: 17px;
    font-weight: normal;
  }

  #recommend_date {
    position: absolute;
    right: 4%;
    margin-top: -90px;
  }

  .td-div {
    margin-bottom: 10px;
  }

  .first-tr td:first-child {
    margin-top: 10px;
  }

  .recommend-table-wrapper .button-div {
    text-align: right;
  }

  .recommend-table-wrapper .first-tr .button-div .primary {
    width: 75px;
    position: absolute;
    margin-top: -60px;
    right: 3%;
    background-color: transparent !important;
    color: rgb(230, 104, 66) !important;
    padding: 0;
  }

  .menu,
  .menu-on {
    font-size: 20px !important;
    padding: 0 6vw !important;
    padding-bottom: 20px !important;
  }

  .list-container {
    margin-top: 30px;
  }

  .list-div {
    flex-direction: column;
    height: 600px;
  }

  .list {
    width: 200px;
  }

  .circle,
  .circle-image {
    width: 200px;
    height: 200px;
  }

  .list-div > div {
    justify-content: center;
  }
}

@media (max-width: 480px) {
  .span-div {
    width: 30%;
    font-size: 17px;
  }

  .recommend-header .div {
    width: 70%;
  }

  .recommend-info span {
    margin: 0;
    padding-right: 15px;
  }

  .list p {
    margin-bottom: 20px;
  }

  .recommend-table-wrapper table {
    margin: 0 auto;
    margin-left: 10px;
    margin-right: 10px;
  }

  .recommend-table-wrapper {
    margin-bottom: 30px;
  }

  .recommend {
    width: 90vw;
    margin-bottom: 50px;
  }

  .recommend-table-wrapper table td {
    padding: 0;
    display: block;
    text-align: left;
    height: 25px;
  }

  .td-div {
    line-height: 1.5;
  }

  .recommend-table-wrapper #recommend_company {
    padding: 0;
    font-size: 14px;
    font-weight: normal;
  }

  .recommend-table-wrapper #recommend_title {
    color: rgb(61, 61, 61);
    font-size: 14px;
    font-weight: normal;
  }

  #recommend_date {
    /* position: absolute;
    right: 4%;
    margin-top: -90px; */
    margin-bottom: 0;
    font-size: 14px;
    margin-top: 20px;
    padding-left: 175px;
  }

  .first-tr td:first-child {
    margin-top: 10px;
  }

  .recommend-table-wrapper .button-div {
    text-align: right;
    height: 10px;
  }

  .recommend-table-wrapper .first-tr .button-div .primary {
    width: 75px;
    position: absolute;
    margin-top: -41px;
    right: 3%;
    background-color: transparent !important;
    color: rgb(230, 104, 66) !important;
    padding: 0;
    font-size: 14px;
    font-weight: normal;
  }

  .menu,
  .menu-on {
    font-size: 13px !important;
    padding: 0 6vw !important;
    padding-bottom: 20px !important;
  }

  .list-container {
    margin-top: 0px;
    margin-bottom: 0;
  }

  .list-div {
    flex-direction: column;
    height: 450px;
  }

  .list {
    width: 130px;
  }

  .circle,
  .circle-image {
    width: 130px;
    height: 130px;
  }

  .list-div > div {
    justify-content: center;
  }
}

@media (max-width: 754px) {
  .menu,
  .menu-on {
    padding: 0 4.6vw !important;
    padding-bottom: 20px !important;
  }
}

/* myresume */

.prof {
  height: 220px;
  width: 190px;
  border: 1px solid #d9d9d9;
  border-right-style: none;
}

.resume-info {
  padding-top: 10px;
  border: 1px solid #d9d9d9;
  padding-left: 40px;
  font-size: 18px;
  font-weight: 400;
  color: rgb(66, 66, 66);
  width: 90%;
}

.resume-info-main #name {
  font-size: 28px;
  color: rgb(66, 66, 66);
}

.resume-info-main {
  font-size: 16px;
  color: #8c8c8c;
  margin: 20px 0;
}

.info-title {
  color: #8c8c8c;
  border-right: 1px solid #bfbfbf;
  margin-right: 15px;
}

.p-title {
  padding-right: 25px;
}

.a-title,
.e-title {
  padding-right: 40px;
}

.b-title {
  padding-right: 23px;
}

.resume-edit {
  width: 75vw;
  margin: 0 auto;
  text-align: right;
  list-style: none;
  margin-top: 30px;
}

.resume-title {
  margin: 0 auto;
  width: 75vw;
}

.resume-title p {
  font-size: 25px;
  font-weight: 400;
  margin-bottom: 15px;
  color: rgb(55, 179, 127);
  margin-top: 20px;
}

.resume-table-wrapper {
  border-top: 2px solid rgb(66, 66, 66);
  display: flex;
  margin: 0 auto;
  width: 75vw;
  margin-bottom: 50px;
}

.resume {
  margin-bottom: 0;
  padding-top: 10px;
  border: 1px solid #d9d9d9;
  padding-left: 40px;
  font-size: 17px;
  font-weight: 400;
  color: rgb(66, 66, 66);
  width: 100%;
}

.resume tbody {
  text-align: center;
}

.resume-type {
  font-weight: 600;
  background-color: rgba(242, 242, 242, 0.5);
}

.resume td {
  border: 1px solid #d9d9d9;
}

.my-resume-info-div {
  display: flex;
}

.my-resume-info-div .div {
  margin-bottom: 10px;
}

.resume-table-wrapper textarea {
  resize: none;
}

@media (max-width: 1154px) {
  .resume-table-wrapper {
    width: 90vw;
  }

  .resume-title {
    width: 90vw;
  }

  .resume-edit {
    width: 90vw;
  }
}

@media (max-width: 929px) {
  .my-resume-info-div {
    flex-direction: column;
  }

  .p-title {
    padding-right: 18px !important;
  }

  .a-title {
    padding-right: 27px !important;
  }

  .e-title {
    padding-right: 19px !important;
  }

  .b-title {
    padding-right: 10px !important;
  }

  .resume td {
    vertical-align: middle;
  }

  .resume-title p {
    font-size: 14px;
    margin-bottom: 0;
  }

  .resume-table-wrapper td {
    font-size: 13px;
  }

  .resume-table-wrapper textarea {
    font-size: 13px;
  }

  .my-resume-info-div .div {
    margin-bottom: 0 !important;
    height: 30px;
  }

  .my-resume-info-div div {
    height: 60px;
    font-size: 15px;
  }

  .resume-edit {
    margin-top: 10px;
  }
}

@media (max-width: 480px) {
  .resume-title {
    margin-top: -30px;
  }

  .resume-table-wrapper .prof,
  .resume-table-wrapper .resume-info {
    height: 160px;
  }

  .resume-info-main {
    margin: 5px 0;
  }

  .resume-edit input {
    width: 90px !important;
    font-size: 10px !important;
    margin-bottom: 10px;
  }

  .resume-info {
    padding-left: 20px;
  }

  .my-resume-info-div div {
    font-size: 10px !important;
  }

  .info-title {
    margin-left: 0 !important;
  }

  .my-resume-info-div .div {
    margin-bottom: 0 !important;
    height: 20px;
  }

  .my-resume-info-div div {
    height: 40px;
  }

  .resume-table-wrapper {
    width: 95vw;
    margin-bottom: 30px;
  }

  .resume-title {
    width: 95vw;
  }

  .resume-edit {
    width: 95vw;
  }
  .resume td {
    font-size: 8px;
  }
  .resume td {
    line-height: 1.5;
    vertical-align: middle;
  }

  .resume-table-wrapper textarea {
    padding-left: 10px;
    height: 200px !important;
    font-size: 8px;
  }
}

/* myinfo */

.my-info-container {
  display: flex;
  margin: 0 auto;
  width: 75vw;
  height: 300px;
  margin-bottom: 20px;
  margin-top: 50px;
}

.my-info-prof {
  width: 250px;
  background-color: rgb(248, 248, 248);
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.my-info-div {
  padding-top: 40px;
  padding-bottom: 40px;
  padding-left: 40px;
  font-size: 18px;
  font-weight: 400;
  color: rgb(66, 66, 66);
  width: 90%;
  background-color: rgb(248, 248, 248);
}

.my-info-div .name {
  font-size: 16px;
  color: #8c8c8c;
  margin-bottom: 2%;
}

.name #name {
  font-size: 28px;
  color: rgb(66, 66, 66);
}

.my-info-content {
  margin-right: 10%;
  margin-bottom: 10px;
}

.my-info-content div {
  margin-bottom: 10px;
}

.my-info-title {
  color: #8c8c8c;
  padding-right: 35px;
  border-right: 1px solid rgb(55, 179, 127);
  margin-right: 15px;
}

.my-info-edit {
  width: 75vw;
  margin: 0 auto;
  text-align: right;
  list-style: none;
}

@media (min-width: 481px) and (max-width: 891px) {
  .my-info-container {
    width: 90vw;
  }

  .my-info-edit {
    width: 90vw;
  }

  .my-info-content {
    margin-right: 0px;
  }

  .my-info-div {
    padding-left: 23px;
  }
}

@media (max-width: 480px) {
  .my-info-prof {
    width: 90vw;
    justify-content: center;
  }

  .my-info-prof img {
    width: 150px;
  }

  .my-info-div {
    width: 90vw;
    padding-top: 0px;
    text-align: center;
    padding-left: 0 !important;
  }

  .my-info-container {
    width: 90vw;
    flex-direction: column;
    height: 400px;
    margin-top: 5px;
    margin-bottom: 10px;
  }

  .my-info-edit {
    width: 90vw;
    text-align: center;
  }

  .my-info-edit a {
    font-size: 13px !important;
  }

  .my-info-content {
    margin-right: 0px;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    padding-left: 20px;
    margin-top: 10px;
  }

  .my-info-content span {
    font-size: 15px;
  }

  .my-info-div {
    padding-left: 23px;
  }

  .name {
    margin: 0;
  }

  #name {
    font-size: 20px !important;
  }
}

/* ----------------------------------------------------- */

/* main Page*/

.main-header,
.main2-header {
  padding: 0 !important;
}

.main-header a {
  color: black !important;
}

.main2-header a {
  color: white !important;
}

.main-warning {
  color: white;
  margin-bottom: 0;
  text-align: center;
}

.main-warning p {
  margin-bottom: 10px;
}

.main-warning i {
  margin-right: 5px;
  color: rgb(230, 104, 66);
}

.search-bar {
  display: flex;
  width: 900px;
}

.search-bar input, .search-bar select {
  width: 40%;
  height: 67px;
  border-radius: 0;
  border: 1px solid white;
  font-size: 20px;
  color: white !important;
  padding-left: 30px !important;
  background-color: rgba(89, 89, 89, 0.5);
  margin: 0;
}

.search-bar input::placeholder {
  color: white !important;
  font-size: 20px;
}

.keyword {
  border-top-left-radius: 30px !important;
  border-bottom-left-radius: 30px !important;
}

.location {
  border-right: none !important;
  border-left: none !important;
}

.job {
  background-color: rgba(89, 89, 89, 0.5);
}

.search-button {
  font-family: FontAwesome !important;

  font-size: 30px;
  color: rgb(61, 61, 61) !important;
  background-color: rgba(255, 255, 255, 0.8) !important;
  border: 1px solid rgba(255, 255, 255, 0.8);
  width: 160px;
  height: 67px;
  border-radius: 0;
  border-top-right-radius: 30px !important;
  border-bottom-right-radius: 30px !important;
  padding: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.inner {
  margin-bottom: 24%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.my-term-div {
  width: 30vw;
}
.my-term {
  margin-top: 7vh;
  background-color: rgba(255, 255, 255, 0.8) !important;
  box-shadow: none;
  font-size: 20px;
  border-radius: 40px;
  color: rgb(55, 179, 127) !important;
}

.logo-div {
  text-align: center;
  padding-top: 7%;
  margin-bottom: 2%;
}

.logo-div img {
  width: 35%;
}

.main-body {
  background-image: url("../../images/bg.jpg");
  background-size: 100vw 100vh;
  height: 100vh;
}

@media screen and (max-width: 980px) {
  .search-bar {
    width: 80%;
  }

  .my-term-div {
    width: 100%;
    margin-top: 0;
  }

  .logo-div {
    padding-top: 100px;
  }
}

@media screen and (max-width: 768px) {
  .main-body {
    background-image: url("../../images/mobile_bg1.jpg");
  }

  .my-term-div {
    width: 100%;
    margin-top: 0;
  }

  .my-term {
    margin-top: 0;
  }

  .search-bar {
    flex-direction: column;
    align-items: center;
    width: 1000px;
  }
  .search-bar input, .search-bar select {
    height: 60px;
  }

  .keyword {
    border-radius: 0 !important;
    border-top-left-radius: 30px !important;
    border-top-right-radius: 30px !important;
  }

  .location {
    border: none !important;
    border-right: 1px solid rgba(255, 255, 255, 0.8) !important;
    border-left: 1px solid rgba(255, 255, 255, 0.8) !important;
  }

  .search-button {
    height: 60px;
    width: 40%;
    border-radius: 0 !important;
    border-bottom-left-radius: 30px !important;
    border-bottom-right-radius: 30px !important;
  }

  .logo-div {
    padding-top: 150px;
  }

  .logo-div img {
    width: 55%;
  }
}

@media screen and (max-width: 480px) {
  .search-bar {
    width: 700px;
  }

  .logo-div img {
    width: 80%;
  }

  .logo-div {
    padding-top: 80px;
  }

  .main-warning p {
    font-size: 12px;
  }

  .search-bar input ,.search-bar select {
    font-size: 17px;
    padding-left: 20px !important;
  }

  .search-bar input::placeholder {
    font-size: 17px;
  }

  .my-term {
    font-size: 15px;
    padding: 0 35px;
  }

  .my-term i {
    font-size: 15px !important;
  }
}

/* join */

.join-logo {
  text-align: center;
  display: flex;
  justify-content: center;
  margin-top: 40px;
  margin-bottom: 25px;
}

.join-logo img {
  width: 45%;
}

.join-type {
  display: flex;
  justify-content: center;
  width: 575px;
  margin: 0 auto;
}

.join-type ul {
  margin-bottom: 0 !important;
  height: 60px;
}
.join-type ul li a {
  height: 60px;
  box-shadow: none !important;
  border-radius: 0;
  font-size: 17px;
}

.join-type ul li a:hover {
  background-color: white;
}

.join-selected li a {
  border-top-left-radius: 20px !important;
  border-top-right-radius: 20px !important;
  background-color: white !important;
}

.join-not-selected li a {
  border-top-right-radius: 20px !important;
  border-top-left-radius: 20px !important;
  border-bottom: 1px solid rgb(179, 179, 179);
  background-color: rgb(242, 242, 242) !important;
