/* commynity main */

.commynity-section {
  display: flex;
  flex-direction: column;
}

.community-banner {
  text-align: left;
  color: rgb(61, 61, 61);
  padding-top: 30px;
  display: flex;
  text-align: center;
  justify-content: center;
  padding-left: 70px;
  /* background-color: rgb(55, 179, 127); */
}

.hashtag {
  font-size: 30px;
  margin-bottom: 40px;
  /* color: rgb(55, 179, 127); */
}

.hashtag {
  font-family: "Sunflower";
  line-height: 4;
}

.community-banner img {
  width: 450px;
  height: 250px;
}

.community-title {
  font-family: "Do Hyeon";
  font-size: 120px;
  margin-top: 20px;
  margin-bottom: 0;
}

.community-btns {
  /* font-family: "Sunflower"; */
  font-family: "Gothic A1";
  display: flex;
  justify-content: center;
  margin-top: 20px;
}

.community-btns div {
  margin: 30px 40px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border: 2px solid rgb(55, 179, 127);
  padding: 20px 0;
}

#ask {
  width: 100px;
}

#review {
  width: 65px;
}

.community-btns a {
  color: rgb(61, 61, 61) !important;
  font-size: 35px;
  padding: 20px 40px;
  border-radius: 15px;
}

@media (max-width: 900px) {
  .hashtag {
    font-size: 25px;
  }

  .community-title {
    font-size: 80px;
  }

  .community-banner img {
    width: 400px;
    height: 200px;
  }
  .community-btns {
    /* font-family: "Sunflower"; */
    font-family: "Gothic A1";
    display: flex;
    justify-content: center;
    margin-top: 20px;
  }

  .community-btns div {
    margin: 30px 40px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    border: 2px solid rgb(55, 179, 127);
    padding: 0px 0;
  }
  .community-btns a {
    font-size: 30px;
  }
  .commynity-section {
    margin-top: 30px;
  }
}

@media (max-width: 480px) {
  .hashtag {
    font-size: 25px;
  }

  .community-banner {
    padding-left: 0px;
    flex-direction: column;
    align-items: center;
    padding-top: 0;
  }

  .community-title {
    font-size: 80px;
    margin-top: 0;
  }

  .community-banner img {
    width: 400px;
    height: 200px;
    margin-top: 10px;
  }
  .community-btns {
    /* font-family: "Sunflower"; */
    font-family: "Gothic A1";
    display: flex;
    justify-content: center;
    margin-top: 20px;
  }

  #ask {
    width: 60px;
  }

  #review {
    width: 40px;
  }

  .community-btns div {
    margin: 0 10px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    border: 2px solid rgb(55, 179, 127);
    padding: 0px;
  }
  .community-btns a {
    font-size: 15px;
    padding: 10px 15px;
  }

  .community-btns a:first-child {
    padding-bottom: 0;
  }
  .commynity-section {
    margin-top: 0px;
  }
}
