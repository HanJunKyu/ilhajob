/* 아래 페이지 넘버 버튼 */
.bbsListTbl {
   width: 100%;
   border-top: 2px solid #384d75;
}

.bbsListTbl th, .bbsListTbl td {
   padding: 10px;
   box-sizing: border-box;
   height: 55px;
}

.bbsListTbl th {
   border-bottom: 1px solid #ccc;
   background-color: #f0f3fa;
}

.bbsListTbl td {
   text-align: center;
   line-height: 1.3;
   border-bottom: 1px solid #ccc;
}

.bbsListTbl td.tit_notice {
   text-align: left;
}

.bbsListTbl td:nth-child(1) {
   width: 10%
}

.bbsListTbl td:nth-child(2) {
   width: 55%
}

.bbsListTbl td:nth-child(3) {
   width: 15%
}

.bbsListTbl td:nth-child(4) {
   width: 20%
}

.pagination {
   padding: 0;
   text-align: center;
   margin-top: -40px;
}

.pagination a {
   display: inline-block;
}

.pagination a.pbtn {
   display: inline-block;
   padding: 7px;
   text-align: center;
   vertical-align: middle;
   font-size: 0;
}

.pagination a .pagenum {
   display: inline-block;
   width: 25px;
   height: 25px;
   line-height: 25px;
   text-align: center;
   vertical-align: top;
   font-size: 20px;
   font-family: '맑은 고딕', 'Malgun Gothic', sans-serif;
}

.pagination a .currentPage {
   border-radius: 3px;
   background: #384d75;
   color: #fff;
}

.pagination a:hover .pagenum {
   border-radius: 3px;
   background-color: #ddd;
}




/* footer css */

html {
  position: relative;

  min-height: 100%;

  margin: 0;
}

body {
  min-height: 100%;
  display:flex;
  flex-direction:column;
}

#review_main{
	display:flex;
 	flex-direction:column;
}


.footer {
  margin-top: 0 auto;
  width: 100%;
  text-align: center;
  color: white;
  background-color: rgb(61, 61, 61);
  display: flex;
  flex-direction: column;
  align-items: center;
  clear:both;
}

.footer > .footer-menu {
  margin-top: 40px;
  height: 30px;
}

.footer > .footer-menu > ul {
  display: flex;
  font-size: 13px;
  color: white;
  padding: 0;
  margin: 0;
}

.footer > .footer-menu > ul > li {
  padding-left: 0;
}

.footer > .footer-menu > ul > li > span {
  padding: 0 8px;
}

.footer > .footer-menu > .labeled-icons:first-child li {
  padding: 0;
}

.footer > .footer-info > ul {
  display: flex;
  font-size: 13px;
  padding: 0;
  margin: 0;
}

.footer > .footer-info > ul > li > span {
  padding: 0 15px;
  border-right: 1px solid #b3b3b3;
}

.copyright {
  margin-bottom: 35px;
}

.footer-info,
.copyright {
  color: rgb(136, 136, 136);
}
