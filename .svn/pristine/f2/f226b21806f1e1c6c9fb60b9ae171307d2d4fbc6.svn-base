package com.ilhajob.app.bbs.vo;

public class BbsVO {
	private int bbs_num;
	private String bbs_title;
	private String bbs_content;
	private String user_id;
	private String bbs_date;
	private int bbs_readcount;
	private String bbs_job;
	private String bbs_category;

	public BbsVO() {;}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public int getBbs_num() {
		return bbs_num;
	}

	public void setBbs_num(int bbs_num) {
		this.bbs_num = bbs_num;
	}

	public String getBbs_date() {
		return bbs_date;
	}

	public void setBbs_date(String bbs_date) {
		this.bbs_date = bbs_date;
	}

	public String getBbs_title() {
		return bbs_title;
	}

	public void setBbs_title(String bbs_title) {
		this.bbs_title = bbs_title;
	}

	public String getBbs_job() {
		return bbs_job;
	}

	public void setBbs_job(String bbs_job) {
		this.bbs_job = bbs_job;
	}

	public String getBbs_category() {
		return bbs_category;
	}

	public void setBbs_category(String bbs_category) {
		this.bbs_category = bbs_category;
	}

	public String getBbs_content() {
		return bbs_content;
	}

	public void setBbs_content(String bbs_content) {
		this.bbs_content = bbs_content;
	}

	public int getBbs_readcount() {
		return bbs_readcount;
	}

	public void setBbs_readcount(int bbs_readcount) {
		this.bbs_readcount = bbs_readcount;
	}
	 
	 
}