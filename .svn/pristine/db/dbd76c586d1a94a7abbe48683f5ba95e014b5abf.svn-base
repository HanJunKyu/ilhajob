package com.ilhajob.app.opening.vo;

public class RecruitmentInfoVO {
	private int opn_num;
	private String recruitment_inf1;
	private String recruitment_inf2;
	private String recruitment_inf3;
	
	public int getOpn_num() {
		return opn_num;
	}
	public void setOpn_num(int opn_num) {
		this.opn_num = opn_num;
	}
	public String getRecruitment_inf1() {
		return recruitment_inf1;
	}
	public void setRecruitment_inf1(String recruitment_inf1) {
		this.recruitment_inf1 = recruitment_inf1;
	}
	public String getRecruitment_inf2() {
		return recruitment_inf2;
	}
	public void setRecruitment_inf2(String recruitment_inf2) {
		this.recruitment_inf2 = recruitment_inf2;
	}
	public String getRecruitment_inf3() {
		return recruitment_inf3;
	}
	public void setRecruitment_inf3(String recruitment_inf3) {
		this.recruitment_inf3 = recruitment_inf3;
	}
	
	
}
